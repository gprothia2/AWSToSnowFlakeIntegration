# Script to create CUSTOMER table in Snowflake database *

create or replace TABLE CUSTOMER (
	C_CUSTKEY VARCHAR(100),
	C_NAME VARCHAR(100),
	C_ADDRESS VARCHAR(100),
	C_CITY VARCHAR(100),
	C_NATION VARCHAR(100),
	C_REGION VARCHAR(100),
	C_PHONE VARCHAR(100),
	C_MKTSEGMENT VARCHAR(100),
	DELETE_FLAG VARCHAR(1),
	LAST_UPDATE_DATE VARCHAR(30)
);
